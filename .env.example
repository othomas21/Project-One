# Supabase Configuration
# Get these from your Supabase project settings: https://supabase.com/dashboard/project/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Server-only Supabase Service Role Key (DO NOT expose to client)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Hugging Face Integration for MedGemma
# Get your API key from: https://huggingface.co/settings/tokens
HUGGING_FACE_API_KEY=hf_your_hugging_face_api_key_here
HUGGING_FACE_TOKEN=hf_your_hugging_face_token_here

# MedGemma Configuration
USE_REAL_MEDGEMMA=false  # Set to 'true' to enable real MedGemma models
NEXT_PUBLIC_ENABLE_AI_FEATURES=true
DEFAULT_MEDGEMMA_MODEL=medgemma-7b

# Python Service Configuration (optional for local inference)
MEDGEMMA_LOCAL_URL=http://localhost:8000
MEDGEMMA_MODEL_ID=RSM-VLM/med-gemma
USE_QUANTIZATION=true

# Model Performance Settings
MEDGEMMA_MAX_TOKENS=512
MEDGEMMA_TEMPERATURE=0.7
MEDGEMMA_CACHE_TTL=300000  # 5 minutes in milliseconds

# Security and Rate Limiting
API_RATE_LIMIT_REQUESTS=100
API_RATE_LIMIT_WINDOW=60000  # 1 minute in milliseconds
ENABLE_REQUEST_LOGGING=true

# Development Settings
DEBUG=false
LOG_LEVEL=info
NODE_ENV=development